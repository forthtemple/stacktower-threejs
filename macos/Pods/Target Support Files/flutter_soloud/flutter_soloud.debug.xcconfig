CLANG_CXX_LANGUAGE_STANDARD = c++17
CLANG_CXX_LIBRARY = libc++
CLANG_WARN_QUOTED_INCLUDE_IN_FRAMEWORK_HEADER = NO
CODE_SIGN_IDENTITY =
CONFIGURATION_BUILD_DIR = ${PODS_CONFIGURATION_BUILD_DIR}/flutter_soloud
DEFINES_MODULE = YES
EXCLUDED_ARCHS[sdk=iphonesimulator*] = i386
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) $(PODS_TARGET_SRCROOT)/include $(PODS_TARGET_SRCROOT)/../src $(PODS_TARGET_SRCROOT)/../src/soloud/include ${PODS_ROOT}/abseil
OTHER_CFLAGS = $(inherited) -O3 -ffast-math -flto
OTHER_CPLUSPLUSFLAGS = -O3 -ffast-math -flto
OTHER_LDFLAGS = $(inherited) -L$(PODS_TARGET_SRCROOT)/libs -l"ogg" -l"opus" -l"vorbis" -l"vorbisfile"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_DEVELOPMENT_LANGUAGE = ${DEVELOPMENT_LANGUAGE}
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/../Flutter/ephemeral/.symlinks/plugins/flutter_soloud/macos
PODS_XCFRAMEWORKS_BUILD_DIR = $(PODS_CONFIGURATION_BUILD_DIR)/XCFrameworkIntermediates
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
VALID_ARCHS = x86_64 arm64
